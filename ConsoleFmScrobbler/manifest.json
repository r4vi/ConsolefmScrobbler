{
  "name": "ConsolefmScrobbler",
  "version": "1.0",
  "description": "Scrobble what you're listening to on Console.fm.",
  "background_page": "background.html",
  "permissions": [
    "http://ws.audioscrobbler.com/*",
	"tabs"
  ],
	"content_scripts": [ {
  		"matches": ["http://console.fm/*"],
  		"js": ["contentscript.js"]
	} 
	]
}
